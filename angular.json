      "root": "projects/debug",
      "sourceRoot": "projects/debug/src",
      "prefix": "debug",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/debug",
            "index": "projects/debug/src/index.html",
            "main": "projects/debug/src/main.ts",
